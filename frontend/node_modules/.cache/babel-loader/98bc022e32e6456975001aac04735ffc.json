;;AAeA,YAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA,IAAA,IAAA,EACA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,OA5BA,EA6BA,KA7BA,CA6BA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,OA/BA;AAgCA;AArEA,IAtCA;AA8GA,EAAA,OAAA,EAAA;AACA,IAAA,UAAA,EAAA,oBAAA,KAAA,EAAA;AACA,UAAA,CAAA,KAAA,EAAA,OAAA,EAAA;AACA,MAAA,KAAA,GAAA,KAAA,CAAA,QAAA,EAAA;AACA,aAAA,KAAA,CAAA,MAAA,CAAA,CAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KALA;AAMA,IAAA,WAAA,EAAA,qBAAA,KAAA,EAAA;AACA,UAAA,CAAA,KAAA,EAAA,OAAA,EAAA;AACA,MAAA,KAAA,GAAA,KAAA,CAAA,QAAA,EAAA;AACA,aAAA,KAAA,CAAA,MAAA,CAAA,CAAA,EAAA,WAAA,EAAA;AACA;AAVA;AA9GA,CAAA","sourcesContent":["<template>\n \n\n      <!-- Main Content -->\n      <div id=\"content\">\n        \n        <!-- Topbar -->\n          <NavbarC> </NavbarC>\n        <!-- End of Topbar -->\n\n        <!-- Begin Page Content -->\n        <div class=\"container-fluid\">\n\n          <!-- Page Heading -->\n          <div class=\"d-sm-flex align-items-center    \">\n            <h1 class=\"h3 mb-0 text-gray-800 p-2\">Les Ecoles </h1> \n              \n          </div>\n         \n              \n          <!-- Content Row -->\n           \n          <!-- Content Row -->\n          <div class=\"row mb-3\" v-if=\"ecole !== null\">\n             \n                <div class=\"col-md-6 text-left\">\n\n                  <div>\n                    <b-card :title=\"ecole.nom_ecole\" :sub-title=\"ecole.complement_ecole\" >\n                      <b-card-text>\n                        {{ecole.descirption}}\n                      </b-card-text>\n\n                      <b-card-text>Type : <span class=\"font-italic\">{{ecole.id_type_ecole}}</span></b-card-text>\n                      <b-card-text>Profils recherchés : <span class=\"font-italic\">GMP / GIM</span></b-card-text>\n                       \n                      <a href=\"#\" class=\"card-link\">Signaler une erreur</a>\n                       \n                    </b-card>\n                  </div>\n                </div>\n                <div class=\"col-md-6\">\n                     \n                      <b-card no-body class=\"full-width\" >\n                      <b-tabs card>\n                        <b-tab title=\"Adresse\"  active >\n                          <iframe :src=\"'https://www.google.com/maps/embed/v1/place?key=AIzaSyA0s1a7phLN0iaD6-UE7m4qP-z21pH0eSc&q='+ecole.adresse.num_rue+'+'+ecole.adresse.nom_rue+'+'+ecole.adresse.ville+'+'+ecole.adresse.pays\" width=\"100%\" height=\"500\" frameborder=\"0\" style=\"border:0\" allowfullscreen></iframe>\n                        </b-tab>\n                        <b-tab title=\"Formation\" >\n                          \n\n                             \n\n                          <ul class=\"list-group\">\n\n                            <li v-for=\"item in ecole.formation\" :key=\"item.id_formation\" class=\"list-group-item d-flex justify-content-between align-items-center\">\n                              {{ item.specialite }}\n                              <b-button type=\"button\" variant=\"primary\" :to=\"'Formation/'+item.id_formation\"><i class=\"fas fa-search\"></i></b-button>\n                            </li>\n  \n                          </ul>\n\n                        </b-tab>\n                        <b-tab title=\"Statistique\" >\n                          <div class=\"text-left\">\n                    <b-card   sub-title=\"2019-2020\">\n                       <b-card-text>Nombre de candidatures : <span class=\"font-italic\">149</span></b-card-text>\n\n                       <b-card-text>Nombre de recurutés : <span class=\"font-italic\">7</span></b-card-text>\n\n                      <b-card-text> Moyenne du dernier : <span class=\"font-italic\">15/20</span></b-card-text>\n                      <b-card-text> Classement du dernier :  <span class=\"font-italic\">7/265</span></b-card-text>\n\n                       \n                    </b-card>\n                  </div>\n                        </b-tab>\n                      </b-tabs>\n                    </b-card>\n                   \n\n\n                    \n                       \n                </div>\n          \n          </div>\n           \n\n          <!-- Content Row -->\n          <div class=\"row\">\n\n            <!-- Content Column -->\n            <div class=\"col-md-8 mb-4\">\n\n              <!-- Project Card Example -->\n              <div class=\"card\">\n                <div class=\"card-body text-light bg-dark\">\n                   <div class=\"row\">\n                      <div class=\"col-sm-3\">Ecole</div>\n                      <div class=\"col-sm-3\">Type</div>\n                      <div class=\"col-sm-4\">Ville</div>\n                      <div class=\"col-sm-2\">Détail</div>\n                  </div>\n                </div>\n              </div>\n              <div v-for=\"item in ecoles\" :key=\"item.id_ecole\" class=\"card mt-1 mb-3 border border-top-0 border-right-0 border-bottom-0 border-danger rounded-0\" >\n                \n                <div class=\"card-body shadow\">\n                   <div class=\"row align-items-center \">\n                      <div class=\"col-sm-3\">{{item.nom_ecole}}</div>\n                      <div class=\"col-sm-3\">{{item.id_type_ecole}}</div>\n                      <div class=\"col-sm-4\">{{item.adresse.ville}}</div>\n                      <div class=\"col-sm-2\"><button type=\"button\" class=\"btn btn-primary\" v-on:click=\"setEcole(item.id_ecole)\"><i class=\"fas fa-search\"></i></button></div>\n                  </div>\n                </div>\n              </div>\n\n               \n \n               \n\n            </div>\n\n            <div class=\"col-md-4 mb-4\">\n\n              <!-- Illustrations -->\n              <div class=\"card shadow mb-4\">\n                <div class=\"card-header py-3\">\n                  <h6 class=\"m-0 font-weight-bold text-primary\">Filtre</h6>\n                </div>\n                <div class=\"card-body\">\n                    <p class=\"d-inline\">\n                         <vue-single-select\n                        name=\"maybe\"\n                placeholder=\"Ecole\"\n                            v-model=\"selectEcole\"\n                            :options=\"ecoleNames\"\n                            \n                    ></vue-single-select>\n                    <vue-single-select\n                        name=\"maybe\"\n                placeholder=\"Formation\"\n                            v-model=\"selectFormation\"\n                            :options=\"formations\"\n                             \n                    ></vue-single-select>\n\n                    <vue-single-select\n                        name=\"maybe\"\n                          placeholder=\"Ville\"\n                            v-model=\"selectedVille\"\n                            :options=\"villes\"\n                            \n                    ></vue-single-select>\n                    \n                    </p>\n                    <button type=\"button\" class=\"btn btn-primary mr-2\">Valider</button>\n                    <button type=\"button\" class=\"btn btn-primary ml-2\">Ajouter une ecole</button>\n                \n                </div>\n              </div>\n\n              <!-- Approach -->\n              \n\n            </div>\n          </div>\n\n        </div>\n        <!-- /.container-fluid -->\n\n      </div>\n      <!-- End of Main Content -->\n\n      \n      \n</template>\n\n<script>\n  import { mapState, mapActions } from 'vuex';\n  import NavbarC from './NavBarCustom.vue';\n  import axios from 'axios'\n  \n  export default {\n    components: {\n    NavbarC\n    },\n    data() {\n      return {\n        email: '',\n        password: '',\n        //ecoles:[{\"nom\":\"INSA\",\"complement\":\"Reseau INSA\",\"description\":\"Haec dum oriens diu perferret, caeli reserato tepore Constantius consulatu suo septies et Caesaris ter egressus Arelate Valentiam petit, in Gundomadum et Vadomarium fratres Alamannorum reges arma moturus, quorum crebris excursibus vastabantur confines limitibus terrae Gallorum.\",\"ville\":\"20 Avenue Albert Einstein, 69100 Villeurbanne\"},{\"nom\":\"IUT\"}],\n        allEcole:[],\n        ecoleNames : [],\n        formations : [],\n        subecoles : [],\n        villes:[],\n        ecole : null,\n        selectEcole:null,\n        selectedVille:null,\n        selectFormation:null\n      }\n    },\n    created () {\n      this.fetchData()\n      \n    },\n    computed: {\n      ...mapState([\n        'loggingIn',\n        'loginError',\n        'accessToken',\n        'logged',\n        'user'\n      ]),\n      ecoles: function(){\n        \n        \n        return this.filtreEcole();\n      }\n    },\n    methods: {\n       filtreEcole(){\n        var f = []\n        this.allEcole.forEach(element => {\n        if((element.nom_ecole.includes(this.selectEcole) || this.selectEcole==null) && \n           (this.selectedVille==null || element.adresse.ville.includes(this.selectedVille) ) && \n           (this.selectEcole == null || element.nom_ecole == this.selectEcole ) \n           ){\n             if(this.selectFormation == null)\n                f.push(element)\n              else{\n                element.formation.forEach(e => {\n                  if( e.specialite.includes(this.selectFormation))\n                    f.push(element)\n                });\n              }\n\n          }\n              \n        });\n        return f;\n      },\n      ...mapActions([\n         \n      ]),\n      setEcole: function(value){\n        this.ecoles.forEach(element => {\n          console.debug(\"id \"+element.id_ecole+\"/\"+value)\n          if(element.id_ecole == value)\n            this.ecole = element\n            \n        });\n        \n        console.debug(\"ecole \"+this.ecole)\n      },\n      fetchData () {\n        console.debug(this.user.mail+\" \"+this.user.pwd)\n        axios({\n            method: 'get',\n            url: 'http://127.0.0.1:5000/api/ecoles/',\n            auth: {\n              username: this.user.mail,\n              password: this.user.pwd\n            }\n        })\n      .then(response => {\n         console.debug(response.data)\n         this.allEcole=response.data\n         this.allEcole.forEach(element => {\n           if(!this.villes.includes(element.adresse.ville))\n              this.villes.push(element.adresse.ville)\n            if(!this.ecoleNames.includes(element.nom_ecole))\n              this.ecoleNames.push(element.nom_ecole)\n            if(element.formation != null){\n              element.formation.forEach(e => {\n              if(!this.formations.includes(e.specialite))\n                  this.formations.push(e.specialite)\n                \n              });\n            }\n\n         });\n\n         if (this.$route.params.id != null)\n            this.setEcole(this.$route.params.id);\n      })\n      .catch(error => {\n        console.debug(error)\n      })\n    }\n    }\n    ,\n    filters: {\n      capitalize: function (value) {\n        if (!value) return ''\n        value = value.toString()\n        return value.charAt(0).toUpperCase() + value.slice(1)\n      },\n      firstLetter: function (value) {\n        if (!value) return ''\n        value = value.toString()\n        return value.charAt(0).toUpperCase() \n      }\n    }\n  }\n</script>\n\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\">\n   \n \n  @import \"../assets/custom.scss\";\n  @import \"node_modules/bootstrap/scss/bootstrap.scss\";\n  @import \"../assets/sb-admin-2.min.css\";\n\n .border {\n    border-width:5px !important;\n}\n \n</style>"],"sourceRoot":"src/components"}]}